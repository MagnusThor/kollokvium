{
  "name": "kollokvium",
  "version": "1.1.3",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "run-electron": "electron electron-launch.js",
    "pack-electron": "",
    "dist-electron": "build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "team kollokvium",


  "build": {
    "appId": "Kollokvium",
    "category": "",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },

  "license": "MIT",
  "dependencies": {
    "@types/bootstrap": "^4.3.1",
    "@types/jquery": "^3.3.33",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "image-capture": "^0.4.0",
    "mediastreamblender": "^1.0.0-beta.4",
    "thor-io.client-vnext": "^2.0.2-beta.2",
    "thor-io.vnext": "^2.0.3-beta.4",
    "webrtc-adapter": "^7.5.1"
  },
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-packager": "^14.2.1"
  }

