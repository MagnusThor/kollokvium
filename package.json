{
  "name": "kollokvium",
  "version": "1.1.3",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "concurrently \"npm run build-client\" \"npm run build-server\"",
    "build-client": "webpack --config webpack.config.js",
    "build-server": "webpack --config webpack.server.js",
    "build-electron": "electron build",
    "start": "node server.js",
    "run-electron": "electron electron-launch.js",
    "pack-electron": "",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "team kollokvium",
  "build": {
    "appId": "Kollokvium",
    "category": "",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@types/bootstrap": "^4.3.1",
    "@types/jquery": "^3.3.33",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "image-capture": "^0.4.0",
    "mediastreamblender": "^1.0.0-beta.4",
    "thor-io.client-vnext": "^2.0.2-beta.2",
    "thor-io.vnext": "^2.0.3-beta.4",
    "webrtc-adapter": "^7.5.1"
  },
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-packager": "^14.2.1",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^4.0.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
